#!/bin/sh
#
# Parse the output of kernel-efi-warnings and write a luv-test-manager
# compatible log to stdout.
awk '/Test:/ {
	test=$2
	printf ("1.0 kernel-efi-warnings %s INFO\r\n", test);
	fflush("");
    }

    /PASS/ {
	printf ("1.0 kernel-efi-warnings %s RESULT 1 0 0 0\r\n", test);
	fflush("");
    }

    /FAIL/ {
	printf ("1.0 kernel-efi-warnings %s RESULT 0 1 0 0\r\n", test);
	fflush("");
    }
'
